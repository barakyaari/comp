NOT:
	PUSH(FP);
	MOV(FP, SP);
	PUSH(R1);
	MOV(R1, 0);
	MOV(R1, FPARG(2));
	CMP(INDD(R1, 0), T_BOOL);
	JUMP_NE(NOT_RETURN_FALSE);
 	CMP(R1, IMM(FALSE));
 	JUMP_EQ(NOT_RETURN_TRUE);
NOT_RETURN_FALSE:
